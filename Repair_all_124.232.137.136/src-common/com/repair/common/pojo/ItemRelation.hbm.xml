<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.repair.common.pojo">
	<class name="ItemRelation" table="item_relation">
		<id name="id" type="integer" column="ID">
			<generator class="native" />
		</id>
		<property name="jcType" column="JCTYPE" type="string"/>
		<property name="firstUnitId" column="FIRSTUNITID" type="integer"/>
		<property name="firstUnitName" column="FIRSTUNITNAME" type="string"/>
		<property name="fixItem" column="FIXITEM" type="string"/>
		<property name="fixContent" column="FIXCONTENT" type="string"/>
		<property name="techStanderd" column="TECHSTANDERD" type="string"/>
		<property name="xcxc" column="XCXC" type="string"/>
		<property name="position" column="POSITION" type="string"/>
		<property name="unit" column="UNIT" type="string"/>
		<property name="itemIds" column="ITEMIDS" type="string"/>
	</class>
</hibernate-mapping>