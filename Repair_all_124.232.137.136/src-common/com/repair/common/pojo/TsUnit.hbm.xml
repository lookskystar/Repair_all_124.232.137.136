<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.repair.common.pojo">
	<class name="TsUnit" table="TS_UNIT">
		<id name="unitId" type="java.lang.Integer">
			<column name="UNITID" />
			<generator class="increment" />
		</id>
		<property name="bzid" type="java.lang.Integer">
			<column name="BZID" />
		</property>		
		<property name="unitName" type="java.lang.String">
			<column name="UNITNAME"></column>
		</property>
	</class>
</hibernate-mapping>
