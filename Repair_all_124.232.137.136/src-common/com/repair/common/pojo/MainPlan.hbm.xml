<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.repair.common.pojo">
	<class name="MainPlan" table="MAINPLAN">
		<id name="id" column="ID">
			<generator class="sequence">
            	<param name="sequence">SEQ_MAINPLAN</param>
            </generator>
		</id>
		<property name="startTime" column="STARTTIME"/>
		<property name="endTime" column="ENDTIME"/>
		<property name="title" column="TITLE"/>
		<property name="makePeople" column="MAKEPEOPLE"/>
		<property name="makeTime" column="MAKETIME"/>
		<property name="comments" column="COMMENTS"/>
		<property name="status" column="STATUS"/>
	</class>
</hibernate-mapping>