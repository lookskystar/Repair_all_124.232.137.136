<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.repair.common.pojo">
	<class name="MonPlanRecorder" table="MONPLAN_RECORDER">
		<id name="monPrecId">
			<generator class="native" />
		</id>
		<property name="jwdCode" />
		<property name="areaId" />
		<many-to-one name="monPlanId"/>
		<property name="jcType" />
		<property name="jcNum" />
		<property name="planFixDate" />
		<property name="befFixDate" />
		<property name="fixFreque" />
		<property name="runKilo" />
		<property name="totalRunKilo" />
		<property name="guessRunKilo" />
		<property name="planStatus" />
		<property name="vary" />
	</class>
</hibernate-mapping>